page {
  width: 100%;
  background: #F6F6F8;
}
.pageCont {
  width: 100%;
}
.pageCont image {
  width: 100%;
  height: 320rpx;
  margin-bottom: 24rpx;
}
.pageCont .addrCont {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  padding: 26rpx 0;
  background: #fff;
}
.pageCont .addrCont .left {
  width: 35%;
  margin-left: 5%;
  color: #333;
  font: 500 30rpx "PingFangSC-Medium";
}
.pageCont .addrCont .right {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  width: 65%;
  margin-right: 5%;
}
.pageCont .addrCont .right text {
  display: inline-block;
  width: 90%;
  text-align: right;
  font-size: 30rpx;
  color: #999;
}
.pageCont .addrCont .right image {
  width: 32rpx;
  height: 32rpx;
  margin: 0;
}
