page {
  background-color: #f5f5f5;
}
.completed {
  width: 100%;
  position: relative;
}
.completed .completedTop {
  width: 100%;
  height: 460rpx;
  background-color: #1a6eff;
  display: flex;
  justify-content: center;
  align-items: center;
}
.completed .statusBar {
  width: 686rpx;
  position: absolute;
  top: 289rpx;
  left: 50%;
  background-color: white;
  transform: translate(-50%, 0);
  border-radius: 16rpx;
  padding: 28rpx;
  box-sizing: border-box;
}
.completed .statusBar .line {
  width: 100%;
  color: #999999;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  margin-bottom: 20rpx;
}
.completed .statusBar .line .leftblock {
  width: 8rpx;
  height: 24rpx;
  border-radius: 4rpx;
  background-color: #1a6eff;
  margin-right: 12rpx;
}
.completed .statusBar .statusPort {
  width: 100%;
  display: flex;
  align-items: flex-end;
  color: #ffcd3a;
  justify-content: center;
  padding-top: 76rpx;
  font-weight: bold;
}
.completed .statusBar .statusPort .port {
  font-size: 120rpx;
  font-weight: bold;
  color: #ffcd3a;
  line-height: 120rpx;
}
.blocklist {
  width: 606rpx;
  height: 160rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #fafafa;
  margin: 0 auto;
  margin-top: 28rpx;
}
.blocklist .item {
  width: 30%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.blocklist .item .title {
  font-size: 32rpx;
  color: #333333;
  font-weight: bold;
}
.blocklist .item .subtitle {
  font-size: 24rpx;
  color: #999999;
}
.blocklist .item1 {
  padding: 0 10rpx;
  box-shadow: 2rpx 0rpx 0rpx 0rpx #dddddd;
}
.blocklist .item2 {
  padding: 0 10rpx;
  box-shadow: 2rpx 0rpx 0rpx 0rpx #dddddd;
}
.bottomTime {
  position: absolute;
  bottom: 60rpx;
  left: 50%;
  transform: translate(-50%, -50%);
  color: #166cff;
  font-size: 28rpx;
}
.completed .statusBar .topBar {
  width: 606rpx;
  height: 160rpx;
  background-color: #fafafa;
  border-radius: 12rpx;
  display: flex;
  align-items: center;
}
.completed .statusBar .topBar .item {
  width: 202rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.completed .statusBar .topBar .item .title {
  font-size: 32rpx;
  color: #333333;
  font-weight: bold;
}
.completed .statusBar .topBar .item .subtitle {
  font-size: 24rpx;
  color: #999999;
}
.completed .detailinfo {
  width: 606rpx;
  margin-top: 25rpx;
}
.completed .detailinfo .item {
  display: flex;
  justify-content: space-between;
  margin-bottom: 10rpx;
}
.completed .detailinfo .item .left {
  color: #999999;
  font-size: 28rpx;
}
.completed .detailinfo .item .right {
  color: #333333;
  font-size: 28rpx;
}
.gotoback {
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translate(-50%,-50%);
  display: flex;
  justify-content: center;
  align-items: center;
  font-weight: bold;
  color: white;
  width: 686rpx;
  height: 100rpx;
  background: linear-gradient(
    270deg,
    rgba(65, 190, 255, 1) 0%,
    rgba(26, 110, 255, 1) 100%
  );
  box-shadow: 0rpx 4rpx 6rpx 0rpx rgba(27, 112, 255, 0.25);
  border-radius: 50rpx;
}
