.pageCont .phoneCont {
  position: fixed;
  left: 0;
  bottom: 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 112rpx;
  background: #fff;
  box-shadow: 0 0 20px rgba(0, 0, 0, 0.1);
}

.pageCont .phoneCont .left {
  margin-left: 5%;
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  flex-direction: column;
  font-size: 24rpx;
  color: #666;
}

.pageCont .phoneCont .right {
  margin-right: 5%;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 296rpx;
  height: 80rpx;
  color: #fff;
  font: 500 28rpx "PingFangSC-Medium";
  border-radius: 44rpx;
  background: linear-gradient(to right, #1a6eff 0%, #41beff 100%) !important;
}

.pageCont .phoneCont .right image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 12rpx;
}

.toTop {
  position: fixed;
  right: 20rpx;
