page {
  background: #F5F5F5;
}

.quitCont {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  width: 100%;
  height: 496rpx;
  background: #fff;
}

.quitCont image {
  width: 160rpx;
  height: 160rpx;
  margin-bottom: 56rpx;
}

.quitCont text:nth-child(2) {
  display: inline-block;
  width: 79%;
  margin-bottom: 32rpx;
  color: #333;
  font: 500 32rpx "PingFangSC-Medium";
  text-align: center;
}

.quitCont text:nth-child(3) {
  color: #999;
  font-size: 28rpx;
}