.registered .login {
  margin-top: 156rpx;
}
.registered .login .loginBtn {
  width: 670rpx;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.registered .login .subtext {
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  color: #576b95;
  margin-top: 41rpx;
  background-color: white;
  border: none;
}
.registered .bottomText {
  left: 50%;
  transform: translate(-50%, 0);
  position: absolute;
  bottom: 50rpx;
  width: 450rpx;
  text-align: center;
  color: #999999;
  font-size: 28rpx;
  display: flex;
  align-items: center;
}
.registered .bottomText .text {
  display: flex;
  flex-wrap: wrap;
  width: 100%;
  justify-content: center;
}
.dialogNoRegistered {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  padding: 48rpx;
  width: 560rpx;
  height: 250rpx;
  border-radius: 8rpx;
  background-color: white;
  display: flex;
  flex-direction: column;
  align-items: center;
  z-index: 1000;
}
.dialogNoRegistered .dialogTop {
  font-size: 36rpx;
  color: #000000;
  line-height: 54rpx;
  font-weight: bold;
  margin-bottom: 28rpx;
}
.dialogNoRegistered .dialogBody {
  font-size: 30rpx;
  padding: 10rpx 0;
  color: #888888;
  display: flex;
  justify-content: center;
  align-items: center;
}
.dialogNoRegistered .bottomBar {
  border-top: 1px solid #e5e5e5;
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #02bb00;
  font-size: 36rpx;
}
.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  background-color: #000;
  z-index: 901;
  top: 0;
  left: 0;
  opacity: 0.5;
}
.show {
  display: block;
}
.noshow {
  display: none;
}
