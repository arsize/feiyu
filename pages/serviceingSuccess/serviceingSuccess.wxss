page {
  background-color: #F7F7F7;
}

.top {
  width: 100%;
  height: 754rpx;
  background-color: white;
}

.top_img {
  width: 160rpx;
  height: 160rpx;
  margin: 0 auto;
  padding-top: 100rpx;
  display: block;
  margin-bottom: 60rpx;

}

.sub_text {
  color: #333333;
  font-size: 36rpx;
  font-weight: bold;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.botton_btn {
  width: 686rpx;
  height: 100rpx;
  background: linear-gradient(270deg, #41BEFF 0%, #1A6EFF 100%);
  border-radius: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  color: white;
  font-weight: bold;
  margin: 0 auto;
  position: absolute;
  bottom: 32rpx;
  left: 50%;
  transform: translate(-50%, -50%);
}