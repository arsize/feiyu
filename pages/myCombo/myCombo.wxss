}

.buyCombo .listWrap .itemMy .right {
  display: flex;
  justify-content: flex-end;
  align-items: center;
  flex-direction: column;
  margin-right: 5%;
}

.buyCombo .listWrap .itemMy .right .rightTop {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.buyCombo .listWrap .itemMy .right .rightTop text {
  color: #999;
  font: 500 24rpx "PingFangSC-Medium";
}

.buyCombo .listWrap .itemMy .right .rightTop image {
  width: 24rpx;
  height: 24rpx;
  /* margin-left: 15rpx; */
}

.buyCombo .listWrap .itemMy .right .rightBtm {
  margin-top: 24rpx;
  font: 500 36rpx "PingFangSC-Medium";
}

