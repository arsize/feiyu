page {
  background-color: #f5f5f5;
}
.waiscroll {
  width: 100%;
  height: 90vh;
  overflow-y: scroll;
}
.waiscroll .chargeinfo .topInfo {
  width: 100%;
  height: 144rpx;
  padding: 32rpx;
  box-sizing: border-box;
  background-color: white;
  margin-bottom: 24rpx;
}
.waiscroll .chargeinfo .topInfo .cabinetName {
  font-size: 32rpx;
  color: #000000;
  line-height: 48rpx;
  font-weight: bold;
}
.waiscroll .chargeinfo .topInfo .item {
  margin-top: 6rpx;
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: #999999;
}
.waiscroll .chargeinfo .centerChargeBox {
  width: 100%;
  background-color: white;
  padding: 32rpx;
  box-sizing: border-box;
  margin-bottom: 24rpx;
}
.waiscroll .chargeinfo .centerChargeBox .toptext {
  font-size: 28rpx;
  color: #666;
}
.waiscroll .chargeinfo .centerChargeBox .cabinetShow {
  width: 100%;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 56rpx;
}
.waiscroll .chargeinfo .centerChargeBox .cabinetBitShow {
  width: 100%;
  box-sizing: border-box;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 56rpx 0;
}
.waiscroll .chargeinfo .centerChargeBox .cabinetBitShow .cabinetItem{
  width: 48%;
}
.waiscroll .chargeinfo .centerChargeBox .cabinetShow .cabinetItem{
  width: 320rpx;
}
.waiscroll .chargeinfo .centerChargeBox  .cabinetItem {
  position: relative;
  border-radius: 8rpx;
  height: 114rpx;
  background-color: #fafafa;
  margin-bottom: 16rpx;
}
.waiscroll .chargeinfo .centerChargeBox  .cabinetItem .portnum {
  font-size: 24rpx;
  color: #999999;
  padding: 10rpx;
}
.waiscroll .chargeinfo .centerChargeBox  .cabinetItem .cabinetCent {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  display: flex;
  align-items: center;
  justify-content: center;
}
.waiscroll
  .chargeinfo
  .centerChargeBox
  .cabinetItem
  .cabinetCent
  .text {
  color: #999999;
  font-size: 28rpx;
  min-width: 84rpx;
  margin-left: 30rpx;
}
.waiscroll .chargeinfo .balanceLine {
  padding: 32rpx;
  background-color: white;
}
.waiscroll .chargeinfo .balanceLine .text {
  font-size: 32rpx;
  color: #333333;
  font-weight: bold;
}
.waiscroll .chargeinfo .balanceLine .advertisement {
  width: 686rpx;
  height: 164rpx;
  margin: 0 auto;
  margin-top: 20rpx;
}
.waiscroll .chargeinfo .balanceLine .advertisement .nowCardWay {
  width: 100%;
  padding: 32rpx;
  box-sizing: border-box;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background-color: #f5f7fa;
}
.waiscroll .chargeinfo .balanceLine .advertisement .nowCardWay .leftBox .top {
  font-size: 30rpx;
  color: #ff9900;
  font-weight: bold;
}
.waiscroll
  .chargeinfo
  .balanceLine
  .advertisement
  .nowCardWay
  .leftBox
  .subinfo {
  font-size: 24rpx;
  color: #999999;
}
.waiscroll .chargeinfo .balanceLine2 {
  height: 120rpx;
  padding: 0 32rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.waiscroll .chargeinfo .balanceLine2 .balanceRight {
  display: flex;
  font-size: 32rpx;
  align-items: center;
}
.totalbottom {
  font-size: 30rpx;
  width: 100%;
  height: 112rpx;
  z-index: 100;
  position: absolute;
  bottom: 0;
  align-items: center;
  display: flex;
  justify-content: space-between;
  box-sizing: border-box;
  background-color: white;
  padding: 0 32rpx;
}
.totalbottom .left {
  color: #ff5532;
  font-weight: bold;
}
.totalbottom .right {
  position: relative;
  width: 280rpx;
  height: 88rpx;
  border-radius: 44rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: white;
}
.totalbottom .right .img {
  position: absolute;
  z-index: 100;
  top: 0;
  left: 0;
}
.totalbottom .right .text {
  position: absolute;
  z-index: 200;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.popTop {
  width: 100%;
  padding: 40rpx;
  display: flex;
  justify-content: space-between;
  box-sizing: border-box;
}
.popLogBody {
  padding: 0 40rpx;
  font-size: 28rpx;
  color: #666666;
}
.popLogBody .row {
  display: flex;
  width: 686rpx;
  height: 62rpx;
  justify-items: center;
  align-items: center;
}
.popLogBody .row1 {
  width: 686rpx;
  height: 62rpx;
  background: rgba(245, 245, 245, 1);
  border-top-right-radius: 12rpx;
  border-top-left-radius: 12rpx;
  margin-top: 10rpx;
  font-size-adjust: 28rpx;
  color: #666666;
}
.exchangeJT {
  width: 686rpx;
  background: rgba(245, 245, 245, 1);
  border-radius: 12rpx;
  padding: 32rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.exchangeJT .title{
  color: #000000;
  font-size-adjust: 30rpx;
  font-weight: bold;
  display: flex;
  align-items: center;
}
.exchangeJT .title .boldtitle{
  font-size: 36rpx;
  color: #196EFF;
  font-weight: bold;
  margin-left: 5rpx;
  margin-right: 5rpx;
}
.popLogBody .row .col {
  width: 50%;
  display: flex;
  justify-content: center;
}
.introst {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 640rpx;
  border-radius: 16rpx;
  background-color: white;
  z-index: 5000;
}
.introst .dialogTop {
  width: 100%;
  height: 100rpx;
  background-color: #fbfbfb;
  border-top-left-radius: 16rpx;
  border-top-right-radius: 16rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.introst .dialogBody {
  width: 100%;
  padding: 30rpx;
  text-align: center;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  color: #666666;
  justify-content: center;
  align-items: center;
}
.introst .dialogBody .item {
  display: flex;
  align-items: center;
  padding-left: 40rpx;
  margin-right: 24rpx;
  width: 100%;
  justify-content: flex-start;
  margin-bottom: 20rpx;
}
.introst .dialogBody .item .left {
}
.introst .dialogBody .item .right {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  color: #666666;
  font-size: 24rpx;
  margin-left: 24rpx;
}
.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  background-color: #000;
  z-index: 4000;
  top: 0;
  left: 0;
  opacity: 0.5;
}
.show {
  display: block;
}
.noshow {
  display: none;
}
.introst .dialogFoot {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0 40rpx;
  box-sizing: border-box;
  padding-bottom: 40rpx;
}
.introst .dialogFoot .item0 {
  width: 400rpx;
  height: 88rpx;
  background: linear-gradient(
    270deg,
    rgba(65, 190, 255, 1) 0%,
    rgba(26, 110, 255, 1) 100%
  );
  box-shadow: 0rpx 4rpx 6rpx 0rpx rgba(27, 112, 255, 0.25);
  border-radius: 44rpx;

  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}
.introst .dialogFoot .item0 .btnbar {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 60%;
  height: 44rpx;
}
.introst .dialogFoot .item0 .text {
  color: white;
  position: absolute;
  left: 50%;
  width: 100%;
  text-align: center;
  top: 50%;
  transform: translate(-50%, -50%);
  z-index: 1500;
}
.popFoot {
  width: 100%;
  height: 140rpx;

  display: flex;
  justify-content: center;
  align-items: center;
}
.popFoot .text {
  width: 400rpx;
  height: 88rpx;
  background: linear-gradient(
    270deg,
    rgba(65, 190, 255, 1) 0%,
    rgba(26, 110, 255, 1) 100%
  );
  box-shadow: 0rpx 4rpx 6rpx 0rpx rgba(27, 112, 255, 0.25);
  border-radius: 44rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: white;
}
